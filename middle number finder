def middle(a, b, c):
  if a == b:
    print(a)
  elif a == c:
    print(a)
  elif b == c:
    print(b)
  else:
    if a < b and a > c or a > b and a < c:
      print(a)
    elif b < a and b > c or b > a and b < c:
      print(b)
    else:
      print(c)

a = int(input("Enter the first number: "))
b = int(input("Enter the second number: "))
c = int(input("Enter the third number: "))

middle(a, b, c)
